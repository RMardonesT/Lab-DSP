%% Representación Gráfica de señales Sinusoidales

t = 0:2:59;
y = sin(t/6);

subplot 311
g1 = stem(t,y,"r");
grid on;
xlabel("tiempo s"); ylabel("Amplitud");
title("Resultado de \texttt{stem} en señal y(t)")

subplot 312
g2 = plot(t,y, "g"); 
grid on
xlabel("tiempo s"); ylabel("Amplitud");
title("Resultado de \texttt{plot} en señal y(t)")

